{
  "name": "rhema",
  "author": "FE",
  "version": "0.8.7",
  "description": "Node framework based on Express.",
  "main": "index.js",
  "scripts": {
    "findleaks": "check-for-leaks",
    "test": "mocha -R spec ./lib/**/tests/**/*-test.js",
    "coverage": "istanbul cover _mocha -- -R spec ./lib/**/tests/**/*-test.js",
    "eslint": "eslint '*.js' lib --ignore-pattern 'lib/**/tests/**'",
    "eslint:fix": "npm run eslint -- --fix",
    "postcoverage": "rm -rf ./build && rm -rf ./coverage",
    "docs": "docsify serve docs",
    "posttest": "rm -rf ./build",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "pre-commit": [
    "findleaks",
    "test"
  ],
  "dependencies": {
    "@risingstack/v8-profiler": "5.7.11",
    "ain2": "2.0.0",
    "async": "1.5.2",
    "basic-auth": "2.0.0",
    "check-for-leaks": "^1.2.0",
    "compression": "1.7.1",
    "continuation-local-storage": "3.2.0",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "csurf": "1.9.0",
    "dateformat": "3.0.2",
    "etag": "1.8.1",
    "express": "4.16.2",
    "fresh": "0.5.2",
    "gleak": "0.5.0",
    "helmet": "3.9.0",
    "html-minifier": "3.5.6",
    "ini": "1.3.4",
    "jade": "1.11.0",
    "js-combinatorics": "0.5.2",
    "json-markup": "1.1.0",
    "lodash": "4.17.5",
    "memwatch-ng": "1.2.0",
    "method-override": "^2.3.10",
    "mkdirp": "0.5.1",
    "moment": "^2.21.0",
    "ms": "2.0.0",
    "node-expat": "^2.3.16",
    "node-statsd": "0.1.1",
    "nodejs-disks": "0.2.1",
    "on-headers": "1.0.1",
    "ouch": "1.0.0",
    "parseurl": "1.3.2",
    "pidusage": "1.2.0",
    "request": "2.83.0",
    "serve-static": "1.13.1",
    "stack-trace": "0.0.10",
    "tracer": "0.8.11",
    "useragent": "2.2.1",
    "uuid": "3.0.1",
    "vhost": "3.0.2",
    "xml2json": "0.11.0",
    "xss": "0.3.4",
    "yamljs": "0.3.0",
    "snyk": "^1.80.1"
  },
  "devDependencies": {
    "chai": "4.1.0",
    "chalk": "2.3.0",
    "docsify": "4.6.8",
    "eslint": "3.19.0",
    "istanbul": "0.4.5",
    "mocha": "3.5.0",
    "node-notifier": "5.1.2",
    "pre-commit": "1.2.2",
    "sinon": "2.4.1"
  },
  "snyk": true
}
